{
  "name": "portfolio_v3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:server": "tsx --watch ./src/data/server.ts",
    "dev": "concurrently -n frontend,server -c green,blue \"npm run dev:frontend\" \"npm run dev:server\"",
    "dev:db": "tsx src/db/index.ts",
